<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.AiNormalQuestionMapper">
    
    <resultMap type="AiNormalQuestion" id="AiNormalQuestionResult">
        <result property="id"    column="id"    />
        <result property="message"    column="message"    />
        <result property="createById"    column="create_by_id"    />
        <result property="createByName"    column="create_by_name"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectAiNormalQuestionVo">
        select id, message, create_by_id, create_by_name, create_time from ai_normal_question
    </sql>

    <select id="selectAiNormalQuestionList" parameterType="AiNormalQuestion" resultMap="AiNormalQuestionResult">
        <include refid="selectAiNormalQuestionVo"/>
        <where>  
            <if test="message != null  and message != ''"> and message = #{message}</if>
            <if test="createById != null "> and create_by_id = #{createById}</if>
        </where>
    </select>
    
    <select id="selectAiNormalQuestionById" parameterType="Long" resultMap="AiNormalQuestionResult">
        <include refid="selectAiNormalQuestionVo"/>
        where id = #{id}
    </select>

    <insert id="insertAiNormalQuestion" parameterType="AiNormalQuestion" useGeneratedKeys="true" keyProperty="id">
        insert into ai_normal_question
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="message != null">message,</if>
            <if test="createById != null">create_by_id,</if>
            <if test="createByName != null">create_by_name,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="message != null">#{message},</if>
            <if test="createById != null">#{createById},</if>
            <if test="createByName != null">#{createByName},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateAiNormalQuestion" parameterType="AiNormalQuestion">
        update ai_normal_question
        <trim prefix="SET" suffixOverrides=",">
            <if test="message != null">message = #{message},</if>
            <if test="createById != null">create_by_id = #{createById},</if>
            <if test="createByName != null">create_by_name = #{createByName},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteAiNormalQuestionById" parameterType="Long">
        delete from ai_normal_question where id = #{id}
    </delete>

    <delete id="deleteAiNormalQuestionByIds" parameterType="String">
        delete from ai_normal_question where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>