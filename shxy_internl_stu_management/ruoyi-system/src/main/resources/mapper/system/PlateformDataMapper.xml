<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.PlateformDataMapper">
    
    <resultMap type="PlateformData" id="PlateformDataResult">
        <result property="id"    column="id"    />
        <result property="dataKey"    column="data_key"    />
        <result property="dataValue"    column="data_value"    />
        <result property="dataDesc"    column="data_desc"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectPlateformDataVo">
        select id, data_key, data_value, data_desc, create_time, update_time from plateform_data
    </sql>

    <select id="selectPlateformDataList" parameterType="PlateformData" resultMap="PlateformDataResult">
        <include refid="selectPlateformDataVo"/>
        <where>  
            <if test="dataKey != null  and dataKey != ''"> and data_key = #{dataKey}</if>
            <if test="dataValue != null  and dataValue != ''"> and data_value = #{dataValue}</if>
            <if test="dataDesc != null  and dataDesc != ''"> and data_desc = #{dataDesc}</if>
        </where>
    </select>
    
    <select id="selectPlateformDataById" parameterType="Long" resultMap="PlateformDataResult">
        <include refid="selectPlateformDataVo"/>
        where id = #{id}
    </select>

    <insert id="insertPlateformData" parameterType="PlateformData" useGeneratedKeys="true" keyProperty="id">
        insert into plateform_data
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dataKey != null">data_key,</if>
            <if test="dataValue != null">data_value,</if>
            <if test="dataDesc != null">data_desc,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dataKey != null">#{dataKey},</if>
            <if test="dataValue != null">#{dataValue},</if>
            <if test="dataDesc != null">#{dataDesc},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updatePlateformData" parameterType="PlateformData">
        update plateform_data
        <trim prefix="SET" suffixOverrides=",">
            <if test="dataKey != null">data_key = #{dataKey},</if>
            <if test="dataValue != null">data_value = #{dataValue},</if>
            <if test="dataDesc != null">data_desc = #{dataDesc},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deletePlateformDataById" parameterType="Long">
        delete from plateform_data where id = #{id}
    </delete>

    <delete id="deletePlateformDataByIds" parameterType="String">
        delete from plateform_data where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>