{
  "name": "pomelox-app-testenv",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "validate-translations": "node package-scripts/validate-translations.js",
    "start": "expo start",
    "start:cache": "expo start --clear",
    "dev": "./scripts/dev-server-manager.sh start",
    "dev:clean": "./scripts/dev-server-manager.sh cleanup && expo start --clear",
    "dev:check": "./scripts/dev-server-manager.sh check",
    "dev:restart": "./scripts/dev-server-manager.sh restart",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:build": "expo export --platform web",
    "build": "expo export --platform web",
    "web:dev": "expo start --web --port 8091",
    "sync:ios": "expo prebuild -p ios --no-install",
    "sync:android": "expo prebuild -p android --no-install",
    "sync": "expo prebuild --no-install",
    "pods": "cd ios && pod install",
    "clean:ios": "rm -rf ios && npm run sync:ios && npm run pods",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --cache",
    "lint:clean": "rm -rf .eslintcache && npm run lint",
    "type-check": "tsc --noEmit --incremental",
    "type-check:clean": "rm -rf .tsbuildinfo && npm run type-check",
    "type-check:watch": "tsc --noEmit --incremental --watch",
    "clean:cache": "rm -rf .eslintcache .tsbuildinfo node_modules/.cache",
    "clean:metro": "npx expo start --clear",
    "clean:all": "npm run clean:cache && npm run clean:metro && npm run clean:ios",
    "version:patch": "node scripts/update-version.js patch",
    "version:minor": "node scripts/update-version.js minor",
    "version:major": "node scripts/update-version.js major",
    "prebuild": "echo 'Skipping type-check and lint for Vercel build'",
    "build:dev": "eas build --platform ios --profile development",
    "build:preview": "eas build --platform ios --profile preview",
    "build:ios": "eas build --platform ios --profile production",
    "submit:ios": "eas submit --platform ios --profile production --latest",
    "release:quick": "eas workflow:run quick-release",
    "release:production": "eas workflow:run production-release",
    "perf:analyze": "npx expo export --dump-assetmap && echo 'Bundle分析完成，检查dist/assetmap.json'",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:ui": "npx playwright test --ui",
    "test:debug": "npx playwright test --debug",
    "test:monitor": "npx playwright test --project=monitor-mode",
    "test:console": "npx playwright test tests/monitoring/console-monitor.spec.ts",
    "test:errors": "npx playwright test tests/monitoring/error-tracker.spec.ts",
    "test:performance": "npx playwright test tests/monitoring/performance-monitor.spec.ts",
    "test:activities": "npx playwright test tests/e2e/activity-flow.spec.ts",
    "test:auth": "npx playwright test tests/e2e/auth-flow.spec.ts",
    "monitor:start": "npm run test:monitor && open monitor-dashboard.html",
    "monitor:dashboard": "open monitor-dashboard.html",
    "monitor:all": "npm run test && npm run monitor:dashboard",
    "console:monitor": "node console-monitor.js",
    "console:monitor:local": "node console-monitor.js http://localhost:8081",
    "console:monitor:prod": "node console-monitor.js https://www.vitaglobal.icu",
    "console:start": "./start-console-monitor.sh",
    "deploy": "node scripts/baota-deploy.js test",
    "deploy:test": "node scripts/baota-deploy.js test",
    "deploy:git": "node scripts/git-deploy.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "expo": "~52.0.0",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-location": "~18.0.10",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "i18next": "^23.15.2",
    "i18next-resources-to-backend": "^1.2.1",
    "intl-pluralrules": "^2.0.1",
    "node-fetch": "^3.3.2",
    "qr-scanner": "^1.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.76.9",
    "react-native-base64": "^0.2.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-localize": "^3.5.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.16.1",
    "react-native-render-html": "^6.2.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/config-plugins": "~9.0.0",
    "@playwright/test": "^1.55.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "archiver": "^7.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-native": "^4.1.0",
    "form-data": "^4.0.4",
    "madge": "^8.0.0",
    "playwright": "^1.55.0",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
